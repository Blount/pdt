<project default="generateParser">

	<target name="generateParser">
		<java 
			jar="../../cup11a/java-cup-11.jar" 
			input="php_ast_parser.cup"
			fork="true">
			<arg value="-compact_red"/>
			<arg value="-expect"/>
			<arg value="4"/>
			<arg value="-parser"/>
			<arg value="PhpAstParser5"/>
			<arg value="-symbols"/>
			<arg value="ParserConstants5"/>
			<arg value="-interface"/>
		</java>

		<move
			file="PhpAstParser5.java"
			todir="../../../../src/org/eclipse/php/internal/core/ast/scanner"/>

		<move
			file="ParserConstants5.java"
			todir="../../../../src/org/eclipse/php/internal/core/ast/scanner"/>

	</target>



</project>
