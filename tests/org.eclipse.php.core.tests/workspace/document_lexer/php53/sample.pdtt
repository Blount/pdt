<?php

    // Returns an indexed array of associative arrays (including keys 'key',
    // 'name', 'desc', 'authorname', 'authorcontact', bool 'package_loaded',
    // array 'modules', array 'loaded_modules') of all currently registered
    // packages
    function EnumeratePackages()
    {
        // The array that's holding our output
        $out = array();

        // Grab a Modules object
        $m = &Modules::singleton();

        // Go through our registry
        foreach ($this->_registry as $package)
        {
            // Create the associative array for this element
            $element = array(
                'key' => $package->key,
                'name' => $package->name,
                'desc' => $package->desc,
                'authorname' => $package->authorname,
                'authorcontact' => $package->authorcontact,
                'package_loaded' =>
                    in_array($this->_loaded_packages, $package->key),
                'modules' => $m->EnumModulesByKey($package->key),
                'modules_loaded' => $m->EnumLoadedModulesByKey($package->key)
                );

            // Add element to output array
            $out[$package->key] = $element;
        }

        // Return output array
        return $out;
    }

