--TEST--
Test multiline array creation inside call chain
--FILE--
<?php
$x = $this->call()->call([1, 2, 3, 4])->call()->call([1, 2, 3, 4]);
?>
--EXPECT--
<?php
$x = $this->call()
    ->call([
        1,
        2,
        3,
        4
    ])
    ->call()
    ->call([
        1,
        2,
        3,
        4
    ]);
?>